/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
view,ul li,image{
  margin:0;
  padding:0;
  list-style:none;
  border:none;
}
pnativegator{
outline: 0;-webkit-text-size-adjust: 100%;-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
.menu-item {
    display: flex;
    flex-direction: column;
    flex:1;
    align-items: center;
    justify-content: center;
    font-size:24rpx;
    color:#aeaeae;
    outline: 0;-webkit-text-size-adjust: 100%;-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

 
.menu-item .img {
  width: 50rpx;
  height: 50rpx;
  display: block;
  margin:8rpx auto 5rpx;
}
 
.clear {
  clear: both;
}
 
.tab-bar {
  width:100%;
  position:fixed;
  bottom:0;
  left:0;
  display:flex;
  align-items: center;
  color:#333333;
  height:100rpx;
  background:#fff;
  box-shadow:6px 0px 13px rgba(117,117,117,0.1);

}