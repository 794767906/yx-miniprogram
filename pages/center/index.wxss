page {
  background-color: #f1f6fb;
  padding-bottom:150rpx;
}

.center_top{
  padding:60rpx 0 60rpx 60rpx;
  overflow:hidden;
  background:#fff url("data:image/jpg;base64,") no-repeat right bottom;
  background-size:208rpx 228rpx;
  margin-bottom:10rpx;
}
.center_top image{
  width:120rpx;
  height:120rpx;
  display:block;
  border-radius:20rpx;
  float:left;
}
.center_top text{
  float:left;
  width:500rpx;
  padding-left:40rpx;
  display:block;
  line-height:70rpx;
  font-size:40rpx;
  color:#333;
}
.center_top text:nth-child(2){
  line-height:40rpx;
  font-size:30rpx;
  color:#bbb;
}
.center_center .tit{
  display:block;
  height:100rpx;
  line-height:100rpx;
  border-bottom:1px solid #f2f7fc;
  padding-left:40rpx;
  font-size:34rpx;
  background:#fff url("data:image/png;base64,") no-repeat 90% center;
  background-size:23rpx 38rpx;
}
.center_center .tit image{
  width:40rpx;
  height:40rpx;
  vertical-align:middle;
  margin-right:20rpx;
}
.center_content{
  width:100%;
  display:flex;
  align-items: center;
  font-size:30rpx;
  color:#333333;
  background:#fff;
  padding:30rpx 0;
  margin-bottom:10rpx;
}
.center_content>view{
   display: flex;
    flex-direction: column;
    flex:1;
    align-items: center;
    justify-content: center;
  position:relative;
}
.center_content>view .size{
  position:absolute;
  right:50rpx;
  top:-6rpx;
  background:#ff3a3a;
  width:30rpx;
  height:30rpx;
  border-radius:50%;
  color:#fff;
  font-size:24rpx;
  text-align: center;
 line-height:30rpx;
}
.center_content>view image{
  width:70rpx;
  height:70rpx;
  margin-bottom:20rpx;
}
.login{
  width:670rpx;
  height:90rpx;
  background:#dddddd;
  border-radius:60rpx;
  margin:50rpx auto 0;
  text-align: center;
  color:#fff;
  line-height:90rpx;
  box-shadow:0px 0px 20px rgba(170,187,205,0.45);
}