/**index.wxss**/
swiper{
  width:100%;
  height:400rpx;
  margin-bottom:20rpx
}
.swiper {
  height: 400rpx;
  width: 100%;
}
.swiper-image {
  height: 100%;
  width: 100%;
}
.slide-image{
   height: 100%;
  width: 100%;
}
swiper .wx-swiper-dots.wx-swiper-dots-horizontal{
     margin-bottom: 2rpx;
}
swiper .wx-swiper-dot{
    width:8px;
    display: inline-flex;
    height:8px; 
    margin-left:7px;
    justify-content:space-between;
}

swiper .wx-swiper-dot-active{
     width:20px;
     height:8px;
     border-radius:20px;
}
.route{
  width:690rpx;
  margin:0 auto;
}
.title{
  text-align: left;
  display:block;
  padding-left:30rpx;
  height:100rpx;
  line-height:100rpx;
  font-size:35rpx;
  color:#333;
  background:url('data:image/png;base64,') no-repeat left center;background-size:8rpx 46rpx;
}
.list{
  width:100%;
  overflow:hidden;
}
.wonderful .list{
  width:340rpx;
  float:left;
}
.wonderful .list .img{
  width:340rpx;
  height:340rpx;
}
.wonderful .list .img image{
  width:340rpx;
  height:340rpx;
}
.route .img{
  position:relative;

}
.route .img .hot{
  position:absolute;
  top:10rpx;
  left:-3rpx;
  width:73rpx;
  height:100rpx;
  z-index:10;
}
.route  .img image{
  width:100%;
  border-radius:20rpx;
}
.list_left{
  width:450rpx;
  float:left;
  font-size:30rpx;
  color:#333;
  margin-left:15rpx;
  margin-bottom:15rpx;
}
.wonderful .list_left{
  width:100%;
  
}
.wonderful .list_left .name{
  line-height:40rpx;
  margin:10rpx 0 15rpx;
  padding:0 10rpx;
}
.list_left .name{
  display:block;
  line-height:50rpx;
}
.list_left .price{
  color:#ff3a3a;
  font-size:40rpx;
  height:60rpx;
 
  
}
.list_left .nowprice{
  line-height:50rpx;
  float:left;
   font-weight:bold;
}
.list_left .yuanprice{
  font-size:24rpx;
  color:#aaaaaa;
  float:left;
  line-height:55rpx;
  margin-left:20rpx;
}
.wonderful .list_left .nowprice{
  color:#aeaeae;
  font-size:36rpx;
}
.wonderful .list:nth-child(even){
  margin-right:10rpx;
}
.wonderful .list_left .people{
  font-size:24rpx;
  color:#aaa;
  float:right;
  margin-right:25rpx;
  line-height:50rpx;
}
.list_left .yuanprice text{
 text-decoration:line-through;
}
.list_right{
  width:220rpx;
  float:right;
}

.list_right image{
  width:135rpx;
  height:75rpx;
  float:right;
  display:block;
}

.list_right text{
  display:block;
  font-size:24rpx;
  line-height: 20rpx;
  text-align:right;
  clear:both;
  color:#aaa;
}
.tab{
  width:100%;
  position:fixed;
  bottom:0;
  left:0;
  display:flex;
  align-items: center;
  font-size:30rpx;
  color:#333333;
  height:110rpx;
  background:#fff;
  box-shadow:0px 0px 10px #999;
}
.wonderful{
  margin-bottom:20rpx;
}
.tab>view{
    align-items: center;
    flex-grow:1;
    text-align: center
}