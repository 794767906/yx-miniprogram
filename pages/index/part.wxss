/* pages/index/part.wxss */
page{
  background:#f1f7fc;
  color:#333;
}
.adress{
 font-size:28rpx;
 background:#fff;
 position:relative;
 padding:25rpx 0 40rpx;
height:210rpx;
 width:100%;
 box-sizing:border-box;
}
.container .zanwei{
  line-height:150rpx;
  padding:0 40rpx;
  font-size:30rpx;
}
.container .zanwei text{
  color:#bbb;
  float:right;
  margin-right:54rpx;
}
.adress .user{
  font-size:38rpx;
  height:70rpx;
  line-height:70rpx;
}
.adress .user text{
  margin-left:20rpx;
}
.adress .place,.adress .user{
  display:block;
  padding:0 40rpx;
}
.adress .place text{
  display:block;
}
.adress>image{
  position:absolute;
 
}
.adress .di_bg{
  width:100%;
  height:6rpx;
  bottom:0;
  left:0;
}
.adress .rightkey{
  width:23rpx;
  height:38rpx;
  right:30rpx;
  top:60rpx;
  padding:20rpx;
}
.adress_con{
  padding:0 20rpx;
  width:100%;
  box-sizing:border-box;
}
.adress_con>view{
  background:#fff;
  margin-top:20rpx;
  border-radius:25rpx;
  padding:20rpx;
 overflow:hidden;
 box-shadow:0 0 20rpx rgba(170,187,205,0.1);
}
.quota image{
  width:120rpx;
  height:120rpx;
  float:left;
  border-radius:20rpx;
}
.quota text{
 display:block;
 float:right;
 font-size:26rpx;
 color:#333;
 line-height:120rpx;
}
.quota text.price{
  float:left;
  font-size:40rpx;
  color:#ff3a3a;
  font-weight:bold;
  line-height:120rpx;
  margin-left:20rpx;
}
.adress_con>view.dateof{
  font-size:28rpx;
  padding-right:0;
  padding-top:0;
}
.dateof>text,.payfor>text,.parent>text,.size>text{
  font-size:30rpx;
  display:block;
  margin-bottom:20rpx;
  font-weight:bold;
  padding-top:20rpx;
  clear:both;
 }
  .size>text{
    float:left;
  }
 .size>view{
   width:200rpx;
   float:right;
   line-height:44rpx;
   height:44rpx;
   margin-top:15rpx;
   margin-right:30rpx;
 }
 .size>view image{
   width:44rpx;
   height:44rpx;
   float:left;
   margin-top:5rpx;
 }
  .size>view input{
    width:112rpx;
    text-align: center;
    float:left;
    line-height:40rpx;
    height:40rpx;
  }
  .parent{
    position:relative;
  }
  .parent image{
    width:44rpx;
    height:44rpx;
    position:absolute;
    right:35rpx;
    top:20rpx;
  }
 .parent>text{
   padding-top:0;
 }
 .parent>view{
   height:80rpx;
   line-height:80rpx;
   font-size:28rpx;
 }
  .parent>view input{
    width:500rpx;
    float:right;
    text-align:right;
    color:#333;
    margin-top:10rpx;
    height:60rpx;
    line-height:60rpx;
  }
 .dateof view{
  width:210rpx;
  height:70rpx;
  background:#f3f3f3;
  float:left;
  border-radius:20rpx;
  margin-right:20rpx;
  margin-bottom:20rpx;
  text-align:center;
  padding:10rpx 0;
  line-height:35rpx;
}
.dateof view text{
  display:block;
  
}
.dateof view.active{
  background:-webkit-linear-gradient(left, #ffdb17 , #ffcb14);
  color:#fff;
}
.dateof view.no{
  color:#bbbbbb;
}
.payfor>text{
  padding-top:0;
}
.payfor{
  font-size:28rpx;
}
.payfor>text{
  margin-bottom:30rpx;
}
.payfor view{
  line-height:70rpx;
  height:70rpx;
  margin-top:20rpx;
  background:url("data:image/png;base64,") no-repeat right center;
  background-size:44rpx 44rpx;
}
.payfor view.active{
  background:url("data:image/png;base64,") no-repeat right center;
  background-size:44rpx 44rpx;
}
.payfor .tishi{
  font-size:22rpx;
  color:#ff3a3a;
  padding:0 20rpx;
  margin-bottom:0;
  font-weight:normal;
  margin-top:6rpx;
}
.payfor view image{
  width:36rpx;
  height:36rpx;
  vertical-align:middle;
  margin-right:10rpx;
}
.student{
  font-size:28rpx;
  display:none;
}
.student .write{
  float:right;
  padding-right:60rpx;
  color:#bbb;
  font-size:26rpx;
  background:url("data:image/png;base64,") no-repeat 85% center;
  background-size:23rpx 38rpx;
}
.adress_con>view.agreed{
  background:none;
  line-height:100rpx;
  height:100rpx;
  font-size:26rpx;
  color:#a1a1a1;
  padding:0;
  margin-top:0;
  padding-left:65rpx;
}
.adress_con>view.agreed text{
  color:#3455cf;
}
.adress_con>view.agreed{
  background:url("data:image/png;base64,") no-repeat 20rpx center;
  background-size:30rpx 30rpx;
}
.adress_con>view.agreed.active{
  background:url("data:image/png;base64,") no-repeat 20rpx center;
  background-size:30rpx 30rpx;
}
.container>button{
  width:100%;
  height:96rpx;
  color:#fff;
  font-size:32rpx;
  background:-webkit-linear-gradient(left, #ffda17 , #ffcb14);
  line-height:96rpx;
  border-radius:0;
}

.hidden{
display: none;
}
.show{
display: block;
}